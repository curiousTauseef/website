---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>bjorkoy.com</title>
 <link href="http://bjorkoy.com/atom.xml" rel="self"/>
 <link href="http://bjorkoy.com/"/>
 
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://bjorkoy.com/</id>
 <author>
   <name>Olav Bjorkoy</name>
   <email>olav@bjorkoy.com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://bjorkoy.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://bjorkoy.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>